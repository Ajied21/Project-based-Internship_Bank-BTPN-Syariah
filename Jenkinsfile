pipeline{
    agent any
    
    stages{
        stage('Build'){
            steps {
                echo 'Sedang membangun project harap bersabar'
            }
        }
        
        stage('Test'){
            steps{
                echo 'Sedang mencoba project, semoga berhasil....:D'
            }
        }
        
        stage('Deploy'){
            steps{
                echo 'Sedang meluncurkan project, semoga sesuai ekpetasi...:3'
            }
        }
    }
}